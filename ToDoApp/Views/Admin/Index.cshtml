@model IEnumerable<ApplicationUser>

<h2>Users</h2>

<table>
    <tr>
        <th>Username</th>
        <th>Email</th>
        <th>Actions</th>
    </tr>
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.UserName</td>
            <td>@user.Email</td>
            <td>
                <a href="@Url.Action("ViewUserToDos", "Admin", new { userId = user.Id })">View To-Do List</a>
            </td>
        </tr>
    }
</table>
