@model IEnumerable<ToDoItem>

<h2>View User To-Do List</h2>

<table>
    <tr>
        <th>Title</th>
        <th>Description</th>
        <th>Status</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Title</td>
            <td>@item.Description</td>
            <td>@item.IsCompleted</td>
        </tr>
    }
</table>
